{
  "version": 2,
  "name": "Stgloc",
  "public": true,
  "trailingSlash": true,
  "cleanUrls": true,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Cache-Control", "value": "public, max-age=300" },
        { "key": "Content-Type", "value": "application/vnd.apple.mpegurl" }
      ]
    }
  ]
}
